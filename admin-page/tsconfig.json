{
  "compilerOptions": {
    "baseUrl": "./src",
    "module": "ESNext",
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "sourceMap": true,
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": false,
    "types": [
      "webpack-env"
    ],
    "skipDefaultLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "paths": {
      "@dev/utils/*": [
        "./utils/*"
      ],
      "@dev/components": [
        "./components"
      ],
      "@dev/pages": [
        "./pages/*"
      ]
    }
  },
  "include": [
    "src"
  ]
}